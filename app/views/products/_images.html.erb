 <div class="productListing">
 <% @products.each do |product| %>
     <div class="col-xs-4">
     	 <ul class="list-unstyled">
	         <li> <%= link_to image_tag(product.image.url(:medium), :size => "200x180"), {:action => 'show', :controller => 'products',  :id => product.id} %> </li>
		     <li><strong> <%= product.title %> </strong></li>
		     <li><p class="prodDescription whitespace"><%= product.description %></p> </li>  
	     </ul>
	</div>
  <% end %>
  </div>
  