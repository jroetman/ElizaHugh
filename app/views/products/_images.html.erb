 <div class="productListing">
 <% @products.each do |product| %>
     <div class="col-sm-4">
     	 <ul class="list-unstyled">
	         <li> <%= link_to image_tag(product.image.url(:medium), :size => "180x170"), {:action => 'show', :controller => 'products',  :id => product.id} %> </li>
		     <li><strong> <%= product.title %> </strong></li>
		     <li><p class="prodDescription whitespace"><%= product.description %></p> </li>  
		     <li><%= itemInQueue(product.id).html_safe %>
	     </ul>
	</div>
  <% end %>
  </div>
  